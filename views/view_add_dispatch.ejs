<!DOCTYPE html>


<html>


<% include ../views/template/stylesheetLinks %>

<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        <% include ../views/template/view_header %>

        <% include ../views/template/nav %>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Dispatch Raw Material
                    <!--<small>Update the inventory.</small>-->
                </h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-dashboard"></i> Dashboard</a></li>
                    <li class="active">Dispatch Raw Material</li>
                </ol>
            </section>

            <!-- Main content -->
            <section class="content">
                <div class="box box-primary">
                    <div>
                        <form action="/raw_material/insert_dispatch" method="POST" class="">
                            <div class="box-body">
                                <div class="form-group" style="width: 25%">
                                    <label>Raw Material Name :</label>
                                    <select class="form-control select2" name="raw_material_name">
                                        <option>--Please Select--</option>
                                        <% if(raw_material_info_list) { %>
                                        <% raw_material_info_list.forEach(function(row){ %>
                                        <option value="<%= row.raw_material_name %>"> <%= row.raw_material_name %>
                                        </option>
                                        <%	}); %>
                                        <% } %>
                                    </select>
                                </div>
                                <div class="form-group" style="width: 25%">
                                    <label>Raw Material Quantity :</label>
                                    <input type="number" step="any" class="form-control" id="raw_material_quantity"
                                        name="raw_material_quantity" placeholder="" required="required">
                                </div>
                                <div class="form-group" style="width: 25%">
                                    <label>From Outlet :</label>
                                    <select class="form-control select2" name="from_outlet">
                                        <option>--Please Select--</option>
                                        <% if(outlet_info_list) { %>
                                        <% outlet_info_list.forEach(function(row){ %>
                                        <option value="<%= row.outlet_name %>">
                                            <%= row.outlet_name %> </option>
                                        <%	}); %>
                                        <% } %>
                                    </select>
                                </div>
                                <div class="form-group" style="width: 25%">
                                    <label>To Outlet :</label>
                                    <select class="form-control select2" name="to_outlet">
                                        <option>--Please Select--</option>
                                        <% if(outlet_info_list) { %>
                                        <% outlet_info_list.forEach(function(row){ %>
                                        <option value="<%= row.outlet_name %>">
                                            <%= row.outlet_name %> </option>
                                        <%	}); %>
                                        <% } %>
                                    </select>
                                </div>
                                <div class="form-group" style="width: 25%">
                                    <label>Unit:</label>
                                    <select class="form-control select2" name="primary_unit">
                                        <option>--Please Select--</option>
                                        <option value="Kg">Kg</option>
                                        <option value="g">g</option>
                                        <option value="l">l</option>
                                    </select>
                                </div>
                            </div>
                            <div class="box-footer">
                                <input type="submit" name="submit" value="Submit" class="btn btn-success">
                            </div>
                        </form>
                    </div>
                </div>
            </section>
        </div>
    </div>
    required
    <% include ../views/template/scripts %>



</body>

</html>