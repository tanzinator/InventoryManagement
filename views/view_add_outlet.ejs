<!DOCTYPE html>


<html>


<% include ../views/template/stylesheetLinks %>

<body class="hold-transition skin-blue sidebar-mini">
	<div class="wrapper">

		<% include ../views/template/view_header %>

		<% include ../views/template/nav %>

		<!-- Content Wrapper. Contains page content -->
		<div class="content-wrapper">
			<!-- Content Header (Page header) -->
			<section class="content-header">
				<h1>
					Add Outlet
					<!--<small>Update the inventory.</small>-->
				</h1>
				<ol class="breadcrumb">
					<li><a href="#"><i class="fa fa-dashboard"></i> Dashboard</a></li>
					<li class="active">Add Outlet</li>
				</ol>
			</section>

			<!-- Main content -->
			<section class="content">
				<div class="box box-primary">
					<div>
						<form action="/outlet/insert_outlet" method="POST" class="">
							<div class="box-body">
								<div class="form-group" style="width: 25%">
									<label>Outlet Name :</label>
									<input type="text" class="form-control" id="outlet_name" name="outlet_name"
										placeholder="" required="required">
								</div>
								<div class="form-group" style="width: 25%">
									<label>Outlet GstIn:</label>
									<input type="text" class="form-control" id="outlet_gstin" name="outlet_gstin"
										placeholder="" required="required">
								</div>
								<div class="form-group" style="width: 25%">
									<label>Outlet Shipping Address :</label>
									<input type="text" class="form-control" id="outlet_shipping_address"
										name="outlet_shipping_address" placeholder="" required="required">
								</div>
								<div class="form-group" style="width: 25%">
									<label>Outlet Phone :</label>
									<input type="number" step="any" class="form-control" id="outlet_phone"
										name="outlet_phone" placeholder="" required="required">
								</div>
								<div class="form-group" style="width: 25%">
									<label>Outlet Email :</label>
									<input type="text" class="form-control" id="outlet_email" name="outlet_email"
										placeholder="" required="required">
								</div>
								<div class="form-group" style="width: 25%">
									<label>Outlet Billing Address :</label>
									<input type="text" class="form-control" id="outlet_billing_address"
										name="outlet_billing_address" placeholder="" required="required">
								</div>
								<div class="box-footer">
									<input type="submit" name="submit" value="Submit" class="btn btn-success">
								</div>

						</form>
					</div>

				</div>
			</section>
		</div>
		required
		<% include ../views/template/scripts %>



</body>

</html>